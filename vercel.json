{
"version": 2,
"functions": {
"api/server.js": {
"runtime": "@vercel/node@latest"
}
},
"rewrites": [
{
"source": "/api/(.)",
"destination": "/api/server.js"
},
{
"source": "/(.)",
"destination": "/index.html"
}
],
"headers": [
{
"source": "/api/(.*)",
"headers": [
{ "key": "Content-Type", "value": "application/json" }
]
}
]
}